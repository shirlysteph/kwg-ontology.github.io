        </owl:propertyChainAxiom>
        <terms:creator>Shirly Stephen</terms:creator>
        <rdfs:label>causes</rdfs:label>
        <skos:definition>Property chaining on the PossiblyCausesRelation class.</skos:definition>
    </owl:ObjectProperty>
    


