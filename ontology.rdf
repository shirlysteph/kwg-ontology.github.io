    <!-- http://www.knowwheregraph.org/ontologies/Hazard#affectedBy -->


    <owl:ObjectProperty rdf:about="http://www.knowwheregraph.org/ontologies/Hazard#affectedBy">
        <owl:inverseOf rdf:resource="http://www.knowwheregraph.org/ontologies/Hazard#affectedElement"/>
